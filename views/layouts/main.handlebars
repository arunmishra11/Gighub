  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>GigHub</title>
  <link rel="stylesheet" href="/css/style.css" />
  <script src="https://cdn.tailwindcss.com"></script>
  <link
    href="https://cdn.jsdelivr.net/npm/daisyui@2.6.0/dist/full.css"
    rel="stylesheet"
    type="text/css"
  />
  <link
    href="https://fonts.googleapis.com/css2?family=Dancing+Script:wght@400..700&family=Give+You+Glory&family=La+Belle+Aurore&family=Oswald:wght@200..700&family=Poppins:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900&family=Rubik:ital,wght@0,300..900;1,300..900&family=Train+One&family=Ubuntu+Sans:ital,wght@0,100..800;1,100..800&display=swap"
    rel="stylesheet"
  />

  <body class="flex flex-col min-h-screen">
    <div
      class="navbar bg-blue-500 h-32 mb-14 border-4 z-[999] flex items-center justify-between w-full"
    >

      <a href="/" class="btn btn-ghost text-xl text-white text-lg">GigHub</a>
      <ul class="menu menu-horizontal px-1">
        <li><a href="/gigRepo" class="text-white text-lg">View Gigs</a></li>
        <li><a href="/gigPost" class="text-white text-lg">Post Gigs</a></li>
		 <!-- Render script for logged in users only -->
      {{#if logged_in}}
        <li><a id="logout" class="text-white text-lg">Logout</a></li>
       {{else}}
        <li><a href="/loginSignup" class="text-white text-lg">Sign Up</a></li>
		{{/if}}
      </ul>
    </div>
	{{{body}}}
     <script src="/js/logout.js"></script>
  
</body>
</html>